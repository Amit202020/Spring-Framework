<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
		"http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
    <bean id="engine" class="com.Engine">
    	<property name="modelNo" value="EB2016"></property>
    </bean>
    <bean id="b" class="com.Car" autowire="constructor">
        <constructor-arg>
        	<value>bmtc</value>
        </constructor-arg>
          <constructor-arg>
        	<value>volvo</value>
        </constructor-arg>
     </bean>
</beans>

